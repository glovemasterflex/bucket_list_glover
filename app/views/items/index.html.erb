<h1><%= @bucket.name %>'s Items</h1>
<% @items.each do |item| %>
<p><%= item.name %><p>
  <%= link_to "Edit", edit_bucket_item_path(@bucket, item) %>
  <%= link_to "Delete", bucket_item_path(@bucket, item), method: :delete %>
<% end %>
<br />
<%= link_to 'Add Item', new_bucket_item_path(@bucket) %>
